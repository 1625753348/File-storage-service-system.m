<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.chromatic.modules.app.dao.CommonDao">

    <select id="selectByTable" resultType="Long">
        select `sp_basic_id` from ${tableName} where id = #{tableId} ;
    </select>

    <select id="selectSpRepair" resultType="Long">
        select
          `sp_repair_history_id`
        from ${tableName}
        where id = #{tableId} ;
    </select>

</mapper>